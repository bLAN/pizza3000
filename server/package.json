{
  "name": "server",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "rimraf ./dist && yarn tsc",
    "tsc": "tsc --project tsconfig.json",
    "tsc:w": "yarn tsc -w",
    "serve": "tsnd --ignore-watch '^/node_modules' src/app.ts",
    "start": "cross-env NODE_ENV=production node dist/src/app.js",
    "dev:e2e": "cross-env NODE_ENV=test tsnd --ignore-watch '^/node_modules' src/app.ts",
    "test": "cross-env NODE_ENV=test mocha --opts \"./tests/mocha.opts\"",
    "test:ci": "cross-env NODE_ENV=test jest --coverage",
    "typeorm": "ts-node -r tsconfig-paths/register ../node_modules/typeorm/cli.js"
  },
  "dependencies": {
    "@tsed/common": "^5.32.1",
    "@tsed/core": "^5.32.1",
    "@tsed/di": "^5.32.1",
    "@tsed/swagger": "^5.32.1",
    "@tsed/typeorm": "^5.32.1",
    "@types/swagger-schema-official": "^2.0.18",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "connect-history-api-fallback": "^1.6.0",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "express": "4",
    "jsonwebtoken": "^9.0.0",
    "method-override": "^3.0.0",
    "passport": "^0.4.0",
    "pg": "^7.12.1",
    "swagger-ui-express": "^4.1.1",
    "ts-httpexceptions": "^4.1.0",
    "typeorm": "^0.2.19",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "@types/compression": "^1.0.1",
    "@types/connect-history-api-fallback": "^1.3.3",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.1",
    "@types/jsonwebtoken": "^8.3.4",
    "@types/method-override": "^0.0.31",
    "@types/node": "^12.7.11",
    "@types/passport": "^1.0.1",
    "@types/uuid": "^3.4.5",
    "nodemon": "^1.19.3",
    "ts-node": "^8.4.1",
    "ts-node-dev": "^1.0.0-pre.43",
    "tsconfig-paths": "^3.9.0",
    "type-fest": "^0.8.1",
    "typescript": "^3.6.3"
  }
}
